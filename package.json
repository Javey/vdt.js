{
  "name": "vdt",
  "version": "0.10.3",
  "description": "A template engine based on virtual-dom",
  "main": "dist/index",
  "module": "src/client",
  "browser": {
      "./src/lib/compile.js": false
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Javey/vdt.js.vdt"
  },
  "license": "MIT",
  "scripts": {
    "build": "rollup -c rollup.config.js && uglifyjs dist/vdt.js -o dist/vdt.min.js -m && rollup -c rollup.config.node.js",
    "test": "mocha --compilers coffee:coffee-script/register test/*.coffee; mocha-phantomjs -p ~/.nvm/versions/node/v4.6.1/bin/phantomjs test/browser/index.html",
    "cover": "istanbul cover _mocha -- --compilers coffee:coffee-script/register test/*.coffee",
    "release": "npm run release-patch",
    "release-patch": "git checkout master && npm version patch && bower version patch 2>/dev/null; git push origin master --tags && npm publish",
    "release-minor": "git checkout master && npm version minor && bower version minor 2>/dev/null; git push origin master --tags && npm publish",
    "release-major": "git checkout master && npm version major && bower version major 2>/dev/null; git push origin master --tags && npm publish"
  },
  "devDependencies": {
    "miss": "*",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "benchmark": "^1.0.0",
    "coffee-script": "^1.9.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "handlebars": "^3.0.2",
    "istanbul": "^0.4.5",
    "jquery": "^2.1.3",
    "mocha": "^2.2.4",
    "mocha-phantomjs": "^4.0.1",
    "mustache": "^2.0.0",
    "platform": "^1.3.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "should": "^11.2.1",
    "todomvc-app-css": "^2.0.1",
    "todomvc-common": "^1.0.1",
    "underscore": "^1.8.3"
  }
}
