{
  "name": "vdt",
  "version": "0.11.0",
  "description": "A template engine based on virtual-dom",
  "main": "src/index",
  "browser": {
    "./src/lib/index.js": "./src/lib/vdt.js",
    "./src/lib/compile.js": "./src/lib/vdt.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Javey/vdt.js.vdt"
  },
  "license": "MIT",
  "scripts": {
    "build": "browserify src/browserify.js -o dist/vdt.js -s Vdt -u src/lib/compile.js && uglifyjs dist/vdt.js -o dist/vdt.min.js -m",
    "test": "mocha --compilers coffee:coffee-script/register test/*.coffee; mocha-phantomjs test/browser/index.html",
    "cover": "istanbul cover _mocha -- --compilers coffee:coffee-script/register test/*.coffee",
    "release": "npm run release-patch",
    "release-patch": "npm version patch && git push origin --tags && npm publish",
    "release-minor": "npm version minor && git push origin --tags && npm publish",
    "release-major": "npm version major && git push origin --tags && npm publish"
  },
  "dependencies": {
    "dom-delegator": "^13.1.0",
    "virtual-domx": "^2.1.12"
  },
  "devDependencies": {
    "benchmark": "^1.0.0",
    "coffee-script": "^1.9.2",
    "handlebars": "^3.0.2",
    "istanbul": "^0.4.5",
    "jquery": "^2.1.3",
    "mocha": "^2.2.4",
    "mocha-phantomjs": "^4.0.1",
    "mustache": "^2.0.0",
    "platform": "^1.3.0",
    "should": "^6.0.1",
    "todomvc-app-css": "^2.0.1",
    "todomvc-common": "^1.0.1",
    "underscore": "^1.8.3"
  }
}
